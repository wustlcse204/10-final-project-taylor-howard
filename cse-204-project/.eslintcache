se {\n            setGame('smash4');\n        }\n    }, [characterName]);\n\n    //use effect to set has details\n    useEffect(() => {\n        if (game === 'smash4') {\n            if (has4Data) {\n                setHasDetails(true);\n            } else {\n                setHasDetails(false);\n            }\n        } else if (game === 'ultimate') {\n            if (hasUltimateData) {\n                setHasDetails(true);\n            } else {\n                setHasDetails(false);\n            }\n        }\n    }, [game]);\n\n    useEffect(() => {\n        if (infoType === 'moves') {\n            const xhttpMovesData = new XMLHttpRequest();\n            xhttpMovesData.onreadystatechange = function () {\n                if (this.readyState === 4 && this.status === 200) {\n                    setMoveData(JSON.parse(this.responseText));\n                }\n            };\n\n            xhttpMovesData.open(\n                'GET',\n                `https://api.kuroganehammer.com/api/characters/${ownerid}/moves?game=${game}`,\n                true,\n            );\n            xhttpMovesData.send();\n        } else if (infoType === 'movements') {\n            const xhttpMovementsData = new XMLHttpRequest();\n            xhttpMovementsData.onreadystatechange = function () {\n                if (this.readyState === 4 && this.status === 200) {\n                    setMovementsData(JSON.parse(this.responseText));\n                }\n            };\n\n            xhttpMovementsData.open(\n                'GET',\n                `https://api.kuroganehammer.com/api/characters/${ownerid}/movements?game=${game}`,\n                true,\n            );\n            xhttpMovementsData.send();\n        }\n    }, [infoType, game, characterName]);\n\n    return (\n        <div className=\"details-page-div\">\n            <div className=\"detail-page-header\" style={{ backgroundColor: color }}>\n                <img src={image} alt={characterName} className=\"detail-header-photo\" />\n                <h2 className=\"text detail-page-character-title\">{characterName}</h2>\n            </div>\n            <div className=\"detail-page-content\">\n                <ul className=\"games-list\">\n                    <li\n                        className={`game-list-item text ${\n                            game === 'smash4' ? 'selected' : 'unselected'\n                        }`}\n                        onClick={() => setGame('smash4')}\n                    >\n                        Smash 4\n                    </li>\n                    <li\n                        className={`game-list-item text ${\n                            game === 'ultimate' ? 'selected' : 'unselected'\n                        }`}\n                        onClick={() => setGame('ultimate')}\n                    >\n                        Ultimate\n                    </li>\n                </ul>\n                <div className=\"details-container\">\n                    <ul className=\"detail-list\">\n                        <li\n                            className={`detail-category text ${\n                                infoType === 'moves' ? 'selected' : 'unselected'\n                            }`}\n                            onClick={() => setInfoType('moves')}\n                        >\n                            Moves\n                        </li>\n                        <li\n                            className={`detail-category text ${\n                                infoType === 'movements' ? 'selected' : 'unselected'\n                            }`}\n                            onClick={() => setInfoType('movements')}\n                        >\n                            Movement Attributes\n                        </li>\n                    </ul>\n                    {hasDetails === false && (\n                        <h1>We do not have this characters data for this game</h1>\n                    )}\n\n                    {hasDetails && infoType === 'moves' && (\n                        <MoveDetails data={moveData} color={color} />\n                    )}\n\n                    {hasDetails && infoType === 'movements' && (\n                        <MovementDetails data={movementsData} color={color} />\n                    )}\n                </div>\n            </div>\n        </div>\n    );\n}\n","/Users/taylorhoward/Developer/CSE204/10-final-project-taylor-howard/cse-204-project/src/Components/MoveDetails/MoveDetails.js",[],"/Users/taylorhoward/Developer/CSE204/10-final-project-taylor-howard/cse-204-project/src/Components/MovementDetails/MovementDetails.js",[],"/Users/taylorhoward/Developer/CSE204/10-final-project-taylor-howard/cse-204-project/src/Components/MoveDataTables/MoveDataTables.js",[],"/Users/taylorhoward/Developer/CSE204/10-final-project-taylor-howard/cse-204-project/src/Components/MovementDataTables/MovementDataTables.js",[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":45,"column":8,"nodeType":"69","endLine":45,"endColumn":10,"suggestions":"70"},{"ruleId":"71","severity":1,"message":"72","line":91,"column":20,"nodeType":"73","messageId":"74","endLine":91,"endColumn":22},{"ruleId":"67","severity":1,"message":"75","line":100,"column":8,"nodeType":"69","endLine":100,"endColumn":72,"suggestions":"76"},{"ruleId":"71","severity":1,"message":"72","line":114,"column":27,"nodeType":"73","messageId":"74","endLine":114,"endColumn":29},{"ruleId":"71","severity":1,"message":"72","line":119,"column":27,"nodeType":"73","messageId":"74","endLine":119,"endColumn":29},{"ruleId":"67","severity":1,"message":"77","line":127,"column":8,"nodeType":"69","endLine":127,"endColumn":16,"suggestions":"78"},{"ruleId":"67","severity":1,"message":"79","line":34,"column":8,"nodeType":"69","endLine":34,"endColumn":23,"suggestions":"80"},{"ruleId":"67","severity":1,"message":"81","line":51,"column":8,"nodeType":"69","endLine":51,"endColumn":14,"suggestions":"